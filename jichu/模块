        # pickle.dump(struct_time1,f)
        # pickle.dump(struct_time2,f)
        # f.close()
        # f = open('pickle_file','rb')
        # struct_time1 = pickle.load(f)
        # struct_time2 = pickle.load(f)
        # print(struct_time1.tm_year)
        # print(struct_time2.tm_year)
        # f.close()


        # dq = deque([1,2])
        # dq.append('a')   # 从后面放数据  [1,2,'a'] 
        #若 dq.append(['a','b'])  则[1,2,['a','b']]
         #若 dq.append('a','b')  则报错
        # dq.appendleft('b') # 从前面放数据 ['b',1,2,'a']
        # print(dq)
        # dq.insert(2,3)    #['b',1,3,2,'a']
        # print(dq.pop())      # 从后面取数据
        # print(dq.popleft())  # 从前面取数据
        # print(dq)

        # 对于字典有两种定义方式：
        # dic={'a':1,'b':2,'c':3}
        # print(dic['a'])
        # d=dict([('a',1),('b',2),('c',3)])
        # print(d['a'])
        # 以上两种定义方式等同

        #有序字典：OrderedDict的Key会按照插入的顺序排列
